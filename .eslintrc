{
  "extends": "airbnb",
  "env": {
    "browser": false,
    "es6": true,
    "node": true
  },
  "rules":{
    "import/no-dynamic-require": 0,
    "callback-return": [0],
    "consistent-return": [0],
    "no-shadow": ["error", { "allow": ["err"] }],
    "object-shorthand": 0,
    "no-param-reassign": 0,
    "no-plusplus": "off"
  },
  "globals": {
    "ga": true,
    "window": true,
    "$": true,
    "jQuery": true,
    "document": true
  },
  "plugins": [
    "jsx-a11y"
  ]
}